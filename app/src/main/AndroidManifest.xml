<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skyreach">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Bottom_Navigation"/>
        <activity android:name=".Forget_Password" />
        <activity android:name=".Side_Navigation"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".Sign_in"
            android:screenOrientation="portrait"/>
        <activity android:name=".signup"
            android:screenOrientation="portrait"/>

    </application>

</manifest>